{
	"name": "woocommerce/collection-stock-filter",
	"version": "1.0.0",
	"title": "Stock Filter",
	"description": "Enable customers to filter the product collection by stock status.",
	"category": "woocommerce",
	"keywords": [ "WooCommerce", "filter", "stock" ],
	"supports": {
		"interactivity": true
	},
	"viewScript": [
		"wc-collection-stock-filter-block-frontend"
	],
	"usesContext": [ "query", "queryId", "postId" ],
	"parent": [ "woocommerce/collection-filters" ],
	"attributes": {
		"className": {
			"type": "string",
			"default": ""
		},
		"headingLevel": {
			"type": "number",
			"default": 3
		},
		"showCounts": {
			"type": "boolean",
			"default": false
		},
		"showFilterButton": {
			"type": "boolean",
			"default": false
		},
		"displayStyle": {
			"type": "string",
			"default": "list"
		},
		"selectType": {
			"type": "string",
			"default": "multiple"
		},
		"isPreview": {
			"type": "boolean",
			"default": false
		}
	},
	"textdomain": "woo-gutenberg-products-block",
	"apiVersion": 2,
	"$schema": "https://schemas.wp.org/trunk/block.json"
}
